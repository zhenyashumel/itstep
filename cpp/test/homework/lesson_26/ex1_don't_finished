#define _CRT_SECURE_NO_WARNINGS
#include<iostream>
#include<cstring>
using namespace std;
const int size = 20;
char max(char a, char b)
{
	if (a > b) return a;
	else return b;

}
char min(char a, char b)
{
	if (a < b) return a;
	else return b;
}
struct LIFO
{
	char stack[size];
	int top;

};
void push(LIFO *st,char ch)
{
	if (st->top == size)
	{
		cout << "Stack is overflowed\n";
		return;
	}
	st->stack[st->top] = ch;
	st->top++;
}
char pop(LIFO *st)
{
	if (st->top == 0)
	{
		cout << "Stack is empty\n";
		return 0;
	}
	st->top--;
	return st->stack[st->top];
}
bool check(char *str)
{
	bool flag = true;
	LIFO st;
	st.top = 0;
	int i = 0;
	while (str[i])
	{
		if (str[i] == '(') push(&st, str[i]);
		else if (str[i] == ')')
		{
			if (st.top != 0)pop(&st);
			else flag = false;
		}
		if (str[i] == '[') push(&st, str[i]);
		else if (str[i] == ']')
		{
			if (st.top != 0)pop(&st);
			else flag = false;
		}
		if (str[i] == '{') push(&st, str[i]);
		else if (str[i] == '}')
		{
			if (st.top != 0)pop(&st);
			else flag = false;
		}

		i++;
	}
	if (st.top != 0)flag = false;
	return flag;


}
	
	int main()
	{
		char str[100], *s, oper[19];
		cin » str;
		if (check(str))cout « "Is right\n";
		else cout « "is wrong\n";
		return 0;
	}
}
